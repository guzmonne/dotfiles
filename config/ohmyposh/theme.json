{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#73daca",
          "properties": {
            "template": "{{ .UserName }} "
          },
          "style": "plain",
          "type": "session"
        },
        {
          "foreground": "#a9b1d6",
          "properties": {
            "style": "full",
            "template": "{{ .Path }} "
          },
          "style": "plain",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#3b4261",
          "properties": {
            "branch_ahead_icon": "<#41a6b5>\u21e1 </>",
            "branch_behind_icon": "<#41a6b5>\u21e3 </>",
            "branch_icon": "",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "github_icon": "",
            "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }}<#ff007c>*</>{{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} "
          },
          "style": "plain",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#6CA35E",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} "
          },
          "style": "powerline",
          "type": "node"
        },
        {
          "foreground": "#9ece6a",
          "properties": {
            "style": "austin",
            "template": " {{ .FormattedMs }} "
          },
          "style": "plain",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#7dcfff",
          "properties": {
            "command": "dotenv -f ~/.config/ohmyposh/.env get OHMYPOSH_KUBERNETES_CONTEXT",
            "shell": "bash",
            "template": " {{ .Output }} "
          },
          "style": "plain",
          "type": "command"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#9d7cd8",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff007c{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "template": "\u276f "
          },
          "style": "plain",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{if .Root}}(Admin){{end}} {{.PWD}}",
  "version": 1
}
